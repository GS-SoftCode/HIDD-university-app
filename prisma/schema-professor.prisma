generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/professor-client"
}

datasource db_professor {
  provider = "postgresql"
}

model Profesor {
  id        Int      @id @default(autoincrement())
  nombre    String
  apellido  String
  correo    String   @unique
  especialidad String
  creadoEn  DateTime @default(now())
}